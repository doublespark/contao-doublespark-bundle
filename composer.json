{
  "name": "doublespark/contao-doublespark-bundle",
  "type": "ds-contao-bundle",
  "description": "Provides custom enhancements for Contao",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamie Devine",
      "homepage": "https://www.doublespark.co.uk"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "contao/core-bundle": "^5.0",
    "league/csv": "^9.8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.2"
  },
  "autoload": {
    "psr-4": {
      "Doublespark\\ContaoDoublesparkBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Doublespark\\ContaoDoublesparkBundle\\ContaoManager\\Plugin"
  }
}